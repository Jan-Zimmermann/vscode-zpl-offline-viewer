{
  "name": "vscode-zpl-viewer",
  "displayName": "VSCode ZPL Viewer",
  "description": "Display ZPL files in VSCode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-zpl-viewer.helloWorld",
    "onCustomEditor:vscode-zpl-viewer.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-zpl-viewer.preview",
        "displayName": "ZPL Preview",
        "selector": [
          {
            "filenamePattern": "*.zpl"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "vscode-zpl-viewer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamilturek/vscode-zpl-viewer"
  },
  "publisher": "Kamil Turek",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.29",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "axios": "^0.27.1"
  }
}
